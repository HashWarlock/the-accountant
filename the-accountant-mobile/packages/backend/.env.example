# Database
DATABASE_URL="postgresql://user:password@localhost:5432/the_accountant_mobile"

# JWT Secret (change in production!)
JWT_SECRET="your-super-secret-jwt-key-change-me"

# App Configuration
APP_NAMESPACE="the-accountant-mobile-dev"
NODE_ENV="development"
PORT=4000

# CORS
CORS_ORIGIN="*"

# TEE Configuration
DSTACK_SOCKET_PATH="/var/run/dstack.sock"

# Attestation
ENABLE_ATTESTATION_UPLOAD="true"
PHALA_CLOUD_API_URL="https://cloud-api.phala.network/api/v1"

# Optional
EXPOSE_INFO="false"
ATTESTATION_UPLOAD_TIMEOUT="30000"
